# Better Auth secret - MUST be the same on auth-server and moshi-server
# Generate with: openssl rand -base64 32
BETTER_AUTH_SECRET=your-secret-here

# PostgreSQL connection string
DATABASE_URL=postgresql://user:password@localhost:5432/moshi_auth

# Auth server port (default: 3001)
AUTH_PORT=3001

# Base URL for Better Auth (used for callbacks, etc.)
BETTER_AUTH_URL=http://localhost:3001
